<?xml version="1.0" encoding="utf-8" ?>
<!-- PURPOSE: NDR Schema is the basis for how information is exchanged from
	Electronic Medical Records (EMRs) to the National Data Repository (NDR) OVERVIEW:
	The NDR Schema is designed to be patient centric and support multiple conditions
	for a Patient. Originally developed by InductiveHealth Informatics, University Maryland Baltimore
	Updated and maintained by PHIS3
	Point of Contact: Dr. Jay Osi Samuels
	(hisupport@phis3project.org.ng) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="unqualified">
    <xs:element name="Container">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="MessageHeader" type="MessageHeaderType"
                            minOccurs="1" maxOccurs="1" />
                <xs:choice>
                    <xs:element name="IndividualReport" type="IndividualReportType"
                                minOccurs="1" maxOccurs="1" />
                </xs:choice>
                <xs:element name="Validation" type="xs:string" minOccurs="0" maxOccurs="1"></xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:complexType name="MessageHeaderType">
        <xs:sequence>
            <xs:element name="MessageStatusCode" minOccurs="1"
                        maxOccurs="1">
                <xs:simpleType>
                    <xs:annotation>
                        <xs:documentation xml:lang="en">
                            The Message Status
                        </xs:documentation>
                    </xs:annotation>
                    <xs:restriction base="CodeType">
                        <xs:enumeration value="INITIAL" />
                        <xs:enumeration value="UPDATED" />
                        <xs:enumeration value="REDACTED" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="MessageCreationDateTime" type="xs:dateTime"
                        minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="en">
                        Message Creation Date
                        and Time
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="MessageSchemaVersion" minOccurs="1"
                        maxOccurs="1" type="xs:string">
            </xs:element>
            <xs:element name="MessageUniqueID" type="StringType"
                        minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="en">
                        The unique identifier
                        for this message. Used to communicate errors and status with
                        Sending Organization.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="MessageSendingOrganization" type="FacilityType"
                        minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="en">
                        The Organization that
                        is sending the message and that is responsible for the facility.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="FacilityType">
        <xs:sequence>
            <xs:element name="FacilityName" type="StringType"
                        minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="en">
                        The Facility Name
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="FacilityID" type="StringType" minOccurs="1"
                        maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="en">
                        The Facility ID that is
                        used to uniquely identify the facility.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="FacilityTypeCode" minOccurs="1"
                        maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="en">
                        The type of facility.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="CodeType">
                        <xs:enumeration value="IP" />
                        <xs:enumeration value="FAC" />
                        <xs:enumeration value="LGA" />
                        <xs:enumeration value="SGA" />
                        <xs:enumeration value="FGA" />
                        <xs:enumeration value="OTH" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="IndividualReportType">
        <xs:sequence>
            <xs:element name="PatientDemographics"
                        type="PatientDemographicsType" minOccurs="1" maxOccurs="1" />
            <xs:element name="Condition" type="ConditionType" minOccurs="1"
                        maxOccurs="unbounded" />
            <xs:element name="HIVTestingReport" type="HIVTestingReportType"
                        minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="PMTCT" type="PMTCTType" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element name="TB" type="TBType" maxOccurs="unbounded" minOccurs="0"></xs:element>
			<xs:element name="Mortality" type="MortalityType" maxOccurs="unbounded" minOccurs="0"></xs:element>
			<xs:element name="Recency" type="RecencyType" maxOccurs="unbounded" minOccurs="0"></xs:element>																						   																					   
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="PMTCTType">
        <xs:sequence>
            <xs:element name="ANCNumber" type="xs:string" minOccurs="0" maxOccurs="1"></xs:element>
            <xs:element name="Immunization" type="ImmunizationType"
                        minOccurs="0" maxOccurs="unbounded" />

            <xs:element name="DeliveryEncounter" type="DeliveryEncounterType"
                        minOccurs="0" maxOccurs="unbounded" />

            <xs:element name="AntenatalRegistration"
                        type="AntenatalRegistrationType" minOccurs="0"
                        maxOccurs="unbounded" />

            <xs:element name="ChildBirthDetails" type="ChildBirthDetailsType"
                        minOccurs="0" maxOccurs="unbounded" />

            <xs:element name="ChildFollowup" type="ChildFollowupType"
                        minOccurs="0" maxOccurs="unbounded" />

            <xs:element name="InfantPCRTesting" type="InfantPCRTestingType"
                        minOccurs="0" maxOccurs="unbounded" />

            <xs:element name="HealthFacilityVisits"
                        type="HealthFacilityVisitsType" minOccurs="0" maxOccurs="unbounded" />

            <xs:element name="PartnerDetails" type="PartnerDetailsType"
                        minOccurs="0" maxOccurs="unbounded" />

            <xs:element name="InfantRapidTest" type="InfantRapidTestType"
                        minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="MaternalCohort" type="MaternalCohortType"
                        minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="PMTCTHTS" type="PMTCTHTSType" maxOccurs="unbounded" minOccurs="0"></xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="TBType">
        <xs:sequence>
            <xs:element name="TBScreeningIPTmonitoring" type="TBScreeningIPTmonitoringType" minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="Screening" type="TBScreeningType" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element name="CommunityAndFacilityReferral" type="TBCommunityAndFacilityReferralType" minOccurs="0" maxOccurs="1" />
            <xs:element name="IndexPatientContactInvestigation" type="TBIndexPatientContactInvestigationType" minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="InterruptionTracking" type="TBInterruptionTrackingType" minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="LaboratoryRegister" type="TBLaboratoryRegisterType" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element name="PatientReferral" type="TBPatientReferralType" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element name="TreatmentMonitoring" type="TBTreatmentMonitoringType" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element name="DRTBTreatmentRegister" type="DRTBTreatmentRegisterType" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element name="DRTBInPatientDischargeForm" type="DRTBInPatientDischargeFormType" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element name="LGAHealthFacilityTBCentralRegister" type="LGAHealthFacilityTBCentralRegisterType" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element name="PresumptiveTBRegister" type="PresumptiveTBRegisterType" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element name="SpecimenExaminationRequestForm" type="SpecimenExaminationRequestFormType" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element name="SpecimenExaminationResultForm" type="SpecimenExaminationResultFormType" maxOccurs="unbounded" minOccurs="0"></xs:element>
           
        </xs:sequence>
    </xs:complexType>

	<xs:complexType name="MortalityType">
		<xs:sequence>
			<xs:element name="VisitID" type="xs:string" minOccurs="1" maxOccurs="1"></xs:element>
			<xs:element name="VisitDate" type="xs:date" minOccurs="1" maxOccurs="1"></xs:element>
			<xs:element name="ReasonForTracking"  minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Reason For Tracking
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="CodeType">
						<xs:enumeration value="CoupTest"></xs:enumeration>
						<xs:enumeration value="MissAppDate"></xs:enumeration>
						<xs:enumeration value="MissPharmDate"></xs:enumeration>
						<xs:enumeration value="Others"></xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="OtherTrackingReason" type="xs:string" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element name="PartnerFullName" type="xs:string" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element name="AddressofTreatmentSupporter" type="xs:string" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element name="ContactPhoneNumber" type="xs:string" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element name="DateofLastActualContact" type="xs:date" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element name="DateofMissedScheduledAppointment" type="xs:date" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element name="DatePatientContacted" type="xs:date" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element name="NameofPersonWhoAttemptedContact" type="xs:string" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element name="ModeofCommunication" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						Mode of Communication
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="CodeType">
						<xs:enumeration value="MobilePhone"></xs:enumeration>
						<xs:enumeration value="HomeVisit"></xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PersonContacted" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						Person Contacted
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="CodeType">
						<xs:enumeration value="Patient"></xs:enumeration>
						<xs:enumeration value="Guardian"></xs:enumeration>
						<xs:enumeration value="TreatmentSupporter"></xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>

			<xs:element name="ReasonforDefaulting"  minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						Reason for Defaulting
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="CodeType">
						<xs:enumeration value="Sick"></xs:enumeration>
						<xs:enumeration value="LackofTransport"></xs:enumeration>
						<xs:enumeration value="PatientForgottoAttendApp"></xs:enumeration>
						<xs:enumeration value="FeltBetandStopedMed"></xs:enumeration>
						<xs:enumeration value="NotPermtoLeaveWork"></xs:enumeration>
						<xs:enumeration value="LostAppCard"></xs:enumeration>
						<xs:enumeration value="StillHadDrugs"></xs:enumeration>
						<xs:enumeration value="HerbalTradMeds"></xs:enumeration>
						<xs:enumeration value="TreatmentSupporter"></xs:enumeration>
						<xs:enumeration value="OthersSpecify"></xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>

			<xs:element name="OtherReasonforDefaulting" type="xs:string" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element name="LosttoFollowup" type="xs:boolean" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element name="ReasonforLosttoFollowup"  minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						Reason for Lost to Follow Up
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="CodeType">
						<xs:enumeration value="DidNotAttempttoTrackPatient"></xs:enumeration>
						<xs:enumeration value="TrackedbutUnabletoLocate"></xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="DateLosttoFollowup" type="xs:date" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element name="PreviousARVExposure"  minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						Previous ARV Exposure
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="CodeType">
						<xs:enumeration value="Yes"></xs:enumeration>
						<xs:enumeration value="No"></xs:enumeration>
						<xs:enumeration value="UNKNOWN"></xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="DateofTermination" type="xs:date" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element name="ReasonforTermination"  minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						Reason For Termination
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="CodeType">
						<xs:enumeration value="TransferredOut"></xs:enumeration>
						<xs:enumeration value="Death"></xs:enumeration>
						<xs:enumeration value="Discontinued"></xs:enumeration>
                        <xs:enumeration value="TreatmentStop"></xs:enumeration>
                        <xs:enumeration value="SelfTransfer"></xs:enumeration>
                        <xs:enumeration value="DuplicateRecord"></xs:enumeration>
                        <xs:enumeration value="CouldNotVerifyClient"></xs:enumeration>
                        <xs:enumeration value="LTFU"></xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
            <xs:element name="IndicationforClientVerification"  minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Indication for Client Verification
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="CodeType">
                        <xs:enumeration value="NoInitialBiometricCapture"></xs:enumeration>
                        <xs:enumeration value="DuplicatedDemographicAndClinicalVariables"></xs:enumeration>
                        <xs:enumeration value="NoBiometricsRecapture"></xs:enumeration>
                        <xs:enumeration value="LastClinicalVisitIsOver15MonthsPrior"></xs:enumeration>
                        <xs:enumeration value="IncompleteVisitData"></xs:enumeration> 
                        <xs:enumeration value="RepeatedClinicalEncounters"></xs:enumeration>
                        <xs:enumeration value="LongIntervalsARVPickup"></xs:enumeration>
                        <xs:enumeration value="SameSexDOBARTStartDate"></xs:enumeration>
						<xs:enumeration value="PickupByProxy"></xs:enumeration>
                        <xs:enumeration value="OtherSpecify"></xs:enumeration>		                        
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
            <xs:element name="ClientVerificationOther" type="xs:string" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element name="TransferredOutTo" type="xs:string" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element name="Death"  minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						Cause of Death
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="CodeType">
						<xs:enumeration value="OtherCauseofDeath"></xs:enumeration>
						<xs:enumeration value="SuspectedARVSideEffect"></xs:enumeration>
						<xs:enumeration value="SuspectedOpportunisticInfection"></xs:enumeration>
						<xs:enumeration value="Unknown"></xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="VACauseofDeath"  minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						VA Cause of Death
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="CodeType">
						<xs:enumeration value="VAAdultCasesofDeath"></xs:enumeration>
						<xs:enumeration value="VAChildCasesofDeath"></xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="OtherCauseofDeath" type="xs:string" minOccurs="0" maxOccurs="1"></xs:element>

			<xs:element name="CauseOfDeath"  minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						Cases of Death
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="CodeType">
						<xs:enumeration value="AIDS"></xs:enumeration>
						<xs:enumeration value="DiarrheaDysentery"></xs:enumeration>
						<xs:enumeration value="Malaria"></xs:enumeration>
						<xs:enumeration value="Maternal"></xs:enumeration>
						<xs:enumeration value="OtherInfetiousDiseases"></xs:enumeration>
						<xs:enumeration value="Pneumonia"></xs:enumeration>
						<xs:enumeration value="TB"></xs:enumeration>
						<xs:enumeration value="AcuteMyocardialInfection"></xs:enumeration>
						<xs:enumeration value="BreastCancer"></xs:enumeration>
						<xs:enumeration value="ChronicRespiratoryDiseases"></xs:enumeration>
						<xs:enumeration value="CervicalCancer"></xs:enumeration>
						<xs:enumeration value="Cirrhosis"></xs:enumeration>
						<xs:enumeration value="ColorectalCancer"></xs:enumeration>
						<xs:enumeration value="Diabetes"></xs:enumeration>
						<xs:enumeration value="EsophagealCancer"></xs:enumeration>
						<xs:enumeration value="LeukamiaLymphomas"></xs:enumeration>
						<xs:enumeration value="LungCancer"></xs:enumeration>
						<xs:enumeration value="OtherCardiovascularDiseases"></xs:enumeration>
						<xs:enumeration value="OtherNonCommunicableDiseases"></xs:enumeration>
						<xs:enumeration value="ProstateCancer"></xs:enumeration>
						<xs:enumeration value="ChronicKidneyDisease"></xs:enumeration>
						<xs:enumeration value="StomachCancer"></xs:enumeration>
						<xs:enumeration value="Stroke"></xs:enumeration>
						<xs:enumeration value="OtherCancers"></xs:enumeration>
						<xs:enumeration value="BiteofVenomousAnimal"></xs:enumeration>
						<xs:enumeration value="Drowning"></xs:enumeration>
						<xs:enumeration value="Falls"></xs:enumeration>
						<xs:enumeration value="Fires"></xs:enumeration>
						<xs:enumeration value="Homicide"></xs:enumeration>
						<xs:enumeration value="OtherInjuries"></xs:enumeration>
						<xs:enumeration value="AccidentalPoisoning"></xs:enumeration>
						<xs:enumeration value="RoadTraffic"></xs:enumeration>
						<xs:enumeration value="SuicideByMultipleMeans"></xs:enumeration>
						<xs:enumeration value="Encephalitis"></xs:enumeration>
						<xs:enumeration value="HemorrhagicFever"></xs:enumeration>
						<xs:enumeration value="Sepsis"></xs:enumeration>
						<xs:enumeration value="Meningitis"></xs:enumeration>
						<xs:enumeration value="Measles"></xs:enumeration>
						<xs:enumeration value="OtherDefinedCausesofChildDeaths"></xs:enumeration>
						<xs:enumeration value="OtherDigestiveDiseases"></xs:enumeration>
						<xs:enumeration value="BirthAsphyxia"></xs:enumeration>
						<xs:enumeration value="CongenitalMalformation"></xs:enumeration>
						<xs:enumeration value="NeonatalMeningitis"></xs:enumeration>
						<xs:enumeration value="NeonatalPneumonia"></xs:enumeration>
						<xs:enumeration value="PretermDelivery"></xs:enumeration>
						<xs:enumeration value="Stillbirth"></xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>

			<xs:element name="DiscontinuedCare"  minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						Discontinued Care
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="CodeType">
						<xs:enumeration value="ForcedDiscontinuation"></xs:enumeration>
						<xs:enumeration value="MovedOutofArea"></xs:enumeration>
						<xs:enumeration value="Other"></xs:enumeration>
						<xs:enumeration value="SelfDiscontinuation"></xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="DiscontinueCareOtherSpecify" type="xs:string" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element name="DateReturnedtoCare" type="xs:date" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element name="ReffferedFor"  minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						Reffered For
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="CodeType">
						<xs:enumeration value="AdherenceCounseling"></xs:enumeration>
						<xs:enumeration value="Other"></xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
            <xs:element name="ReffferedForOther" type="xs:string" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element name="NameofContactTracer" type="xs:string" minOccurs="0" maxOccurs="1"></xs:element>
			<xs:element name="ContactTrackerSignatureDate" type="xs:date" minOccurs="0" maxOccurs="1"></xs:element>

		</xs:sequence>
	</xs:complexType>
	
    <xs:complexType name="ConditionType">
        <xs:sequence>
            <xs:element name="ConditionCode" type="CodeType" minOccurs="1" maxOccurs="1" />
            <xs:element name="ProgramArea" type="ProgramAreaType" minOccurs="1" maxOccurs="1" />
            <xs:element name="PatientAddress" type="AddressType" minOccurs="0" maxOccurs="1" />
            <xs:element name="CommonQuestions" type="CommonQuestionsType" minOccurs="0" maxOccurs="1" />
            <xs:element name="ConditionSpecificQuestions" type="ConditionSpecificQuestionsType" minOccurs="0" maxOccurs="1" />
            <xs:element name="Encounters" type="EncountersType" minOccurs="0" maxOccurs="1" />
            <xs:element name="LaboratoryReport" type="LaboratoryReportType" minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="Regimen" type="RegimenType" minOccurs="0" maxOccurs="unbounded" />

            <xs:element name="EAC" type="EACType" maxOccurs="unbounded" minOccurs="0"></xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ConditionSpecificQuestionsType">
        <xs:choice>
            <xs:element name="HIVQuestions" type="HIVQuestionsType"
                        minOccurs="0" maxOccurs="1" />
        </xs:choice>
    </xs:complexType>
    <xs:complexType name="HIVQuestionsType">
        <xs:sequence>
            <xs:element name="CareEntryPoint" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="en">
                        Code that represents the Care Entry Point for patient into HIV care
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="CodeType">
                        <xs:enumeration value="1" />
                        <xs:enumeration value="2" />
                        <xs:enumeration value="3" />
                        <xs:enumeration value="4" />
                        <xs:enumeration value="5" />
                        <xs:enumeration value="6" />
                        <xs:enumeration value="7" />
                        <xs:enumeration value="8" />
                        <xs:enumeration value="9" />
                        <xs:enumeration value="10" />
                        <xs:enumeration value="11" />
                        <xs:enumeration value="12" />
                        <xs:enumeration value="13" />
                        <xs:enumeration value="14" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="FirstConfirmedHIVTestDate" type="xs:date" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Date of First Confirmed HIV test</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="FirstHIVTestMode" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="en">
