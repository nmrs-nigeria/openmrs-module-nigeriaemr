<?xml version="1.0" encoding="utf-8" ?>
<!-- PURPOSE: NDR Schema is the basis for how information is exchanged from 
	Electronic Medical Records (EMRs) to the National Data Repository (NDR) OVERVIEW: 
	The NDR Schema is designed to be patient centric and support multiple conditions 
	for a Patient. Originally developed by InductiveHealth Informatics, University Maryland Baltimore
	Updated and maintained by PHIS3
	Point of Contact: Dr. Jay Osi Samuels 
	(hisupport@phis3project.org.ng) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	elementFormDefault="unqualified">
  <xs:element name="Container">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="MessageHeader" type="MessageHeaderType"
					minOccurs="1" maxOccurs="1" />
        <xs:choice>
          <xs:element name="IndividualReport" type="IndividualReportType"
						minOccurs="1" maxOccurs="1" />
        </xs:choice>
        <xs:element name="Validation" type="xs:string" minOccurs="0" maxOccurs="1"></xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="MessageHeaderType">
    <xs:sequence>
      <xs:element name="MessageStatusCode" minOccurs="1"
				maxOccurs="1">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation xml:lang="en">
              The Message Status
            </xs:documentation>
          </xs:annotation>
          <xs:restriction base="CodeType">
            <xs:enumeration value="INITIAL" />
            <xs:enumeration value="UPDATED" />
            <xs:enumeration value="REDACTED" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="MessageCreationDateTime" type="xs:dateTime"
				minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Message Creation Date
            and Time
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MessageSchemaVersion" minOccurs="1"
				maxOccurs="1" type="xs:string">    
      </xs:element>
      <xs:element name="MessageUniqueID" type="StringType"
				minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The unique identifier
            for this message. Used to communicate errors and status with
            Sending Organization.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MessageSendingOrganization" type="FacilityType"
				minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The Organization that
            is sending the message and that is responsible for the facility.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FacilityType">
    <xs:sequence>
      <xs:element name="FacilityName" type="StringType"
				minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The Facility Name
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FacilityID" type="StringType" minOccurs="1"
				maxOccurs="1">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The Facility ID that is
            used to uniquely identify the facility.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FacilityTypeCode" minOccurs="1"
				maxOccurs="1">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The type of facility.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="CodeType">
            <xs:enumeration value="IP" />
            <xs:enumeration value="FAC" />
            <xs:enumeration value="LGA" />
            <xs:enumeration value="SGA" />
            <xs:enumeration value="FGA" />
            <xs:enumeration value="OTH" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="IndividualReportType">
    <xs:sequence>
    	<xs:element name="PatientDemographics"
    		type="PatientDemographicsType" minOccurs="1" maxOccurs="1" />
    	<xs:element name="Condition" type="ConditionType" minOccurs="1"
    		maxOccurs="unbounded" />
    	<xs:element name="HIVTestingReport" type="HIVTestingReportType"
    		minOccurs="0" maxOccurs="unbounded" />
    	<xs:element name="PMTCT" type="PMTCTType" maxOccurs="unbounded"
    		minOccurs="0">
    	</xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PMTCTType">
  <xs:sequence>
  <xs:element name="ANCNumber" type="xs:string" minOccurs="0" maxOccurs="1"></xs:element>
  	<xs:element name="Immunization" type="ImmunizationType"
  		minOccurs="0" maxOccurs="unbounded" />

  	<xs:element name="DeliveryEncounter" type="DeliveryEncounterType"
  		minOccurs="0" maxOccurs="unbounded" />

  	<xs:element name="AntenatalRegistration"
  		type="AntenatalRegistrationType" minOccurs="0"
  		maxOccurs="unbounded" />

  	<xs:element name="ChildBirthDetails" type="ChildBirthDetailsType"
  		minOccurs="0" maxOccurs="unbounded" />

  	<xs:element name="ChildFollowup" type="ChildFollowupType"
  		minOccurs="0" maxOccurs="unbounded" />

  	<xs:element name="InfantPCRTesting" type="InfantPCRTestingType"
  		minOccurs="0" maxOccurs="unbounded" />

  	<xs:element name="HealthFacilityVisits"
  		type="HealthFacilityVisitsType" minOccurs="0" maxOccurs="unbounded" />

  	<xs:element name="PartnerDetails" type="PartnerDetailsType"
  		minOccurs="0" maxOccurs="unbounded" />

  	<xs:element name="InfantRapidTest" type="InfantRapidTestType"
  		minOccurs="0" maxOccurs="unbounded" />
  	<xs:element name="MaternalCohort" type="MaternalCohortType"
  		minOccurs="0" maxOccurs="unbounded" />
  	<xs:element name="PMTCTHTS" type="PMTCTHTSType" maxOccurs="unbounded" minOccurs="0"></xs:element>
	  <xs:element name="PMTCTRegistration" type="PMTCTRegistrationType" maxOccurs="unbounded" minOccurs="0"></xs:element>
  </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ConditionType">
    <xs:sequence>
    	<xs:element name="ConditionCode" type="CodeType" minOccurs="1" maxOccurs="1" />
    	<xs:element name="ProgramArea" type="ProgramAreaType" minOccurs="1" maxOccurs="1" />
    	<xs:element name="PatientAddress" type="AddressType" minOccurs="0" maxOccurs="1" />
    	<xs:element name="CommonQuestions" type="CommonQuestionsType" minOccurs="0" maxOccurs="1" />
    	<xs:element name="ConditionSpecificQuestions" type="ConditionSpecificQuestionsType" minOccurs="0" maxOccurs="1" />
    	<xs:element name="Encounters" type="EncountersType" minOccurs="0" maxOccurs="1" />
    	<xs:element name="LaboratoryReport" type="LaboratoryReportType" minOccurs="0" maxOccurs="unbounded" />
    	<xs:element name="Regimen" type="RegimenType" minOccurs="0" maxOccurs="unbounded" />
	</xs:sequence>
  </xs:complexType>
  <xs:complexType name="ConditionSpecificQuestionsType">
    <xs:choice>
      <xs:element name="HIVQuestions" type="HIVQuestionsType"
				minOccurs="0" maxOccurs="1" />
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="HIVQuestionsType">
    <xs:sequence>
    	<xs:element name="CareEntryPoint" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">
    				Code that represents the Care Entry Point for patient into HIV care
    			</xs:documentation>
    		</xs:annotation>
    		<xs:simpleType>
    			<xs:restriction base="CodeType">
    				<xs:enumeration value="1" />
    				<xs:enumeration value="2" />
    				<xs:enumeration value="3" />
    				<xs:enumeration value="4" />
    				<xs:enumeration value="5" />
    				<xs:enumeration value="6" />
    				<xs:enumeration value="7" />
    				<xs:enumeration value="8" />
    				<xs:enumeration value="9" />
    				<xs:enumeration value="10" />
    				<xs:enumeration value="11" />
    				<xs:enumeration value="12" />
    				<xs:enumeration value="13" />
    				<xs:enumeration value="14" />
    			</xs:restriction>
    		</xs:simpleType>
    	</xs:element>
    	<xs:element name="FirstConfirmedHIVTestDate" type="xs:date" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">Date of First Confirmed HIV test</xs:documentation>
    		</xs:annotation>
    	</xs:element>
    	<xs:element name="FirstHIVTestMode" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">
    				Code that represents the type of first confirmed HIV test as [HIV-Ab] or [PCR]
    			</xs:documentation>
    		</xs:annotation>
    		<xs:simpleType>
    			<xs:restriction base="CodeType">
    				<xs:enumeration value="HIVAb" />
    				<xs:enumeration value="HIVPCR" />
    			</xs:restriction>
    		</xs:simpleType>
    	</xs:element>
    	<xs:element name="WhereFirstHIVTest" type="StringType" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">Description of where the first HIV test occurred</xs:documentation>
    		</xs:annotation>
    	</xs:element>
    	<xs:element name="PriorArt" type="CodeType" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">Type of prior ART</xs:documentation>
    		</xs:annotation>
    	</xs:element>
    	<xs:element name="MedicallyEligibleDate" type="xs:date" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">Date of Patient became medically eligible for ART</xs:documentation>
    		</xs:annotation>
    	</xs:element>
    	<xs:element name="ReasonMedicallyEligible" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">Why Eligible for ART</xs:documentation>
    		</xs:annotation>
    		<xs:simpleType>
    			<xs:restriction base="CodeType">
    				<xs:enumeration value="1" />
    				<xs:enumeration value="2" />
    				<xs:enumeration value="3" />
    				<xs:enumeration value="4" />
    			</xs:restriction>
    		</xs:simpleType>
    	</xs:element>
    	<xs:element name="InitialAdherenceCounselingCompletedDate" type="xs:date" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">Date Initial Adherence Counseling Completed</xs:documentation>
    		</xs:annotation>
    	</xs:element>
    	<xs:element name="PatientTransferredIn" type="xs:boolean" minOccurs="0" maxOccurs="1"></xs:element>
    	<xs:element name="TransferredInDate" type="xs:date" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">Date initially transferred in to ARV care</xs:documentation>
    		</xs:annotation>
    	</xs:element>
    	<xs:element name="TransferredInFrom" type="FacilityType" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">Facility where patient transferred from</xs:documentation>
    		</xs:annotation>
    	</xs:element>
    	<xs:element name="TransferredInFromPatId" minOccurs="0" maxOccurs="1">
    		<xs:simpleType>
    			<xs:annotation>
    				<xs:documentation xml:lang="en">The Patient's Identifier from the previous facility</xs:documentation>
    			</xs:annotation>
    			<xs:restriction base="xs:string">
    				<xs:minLength value="0" />
    				<xs:maxLength value="2000" />
    			</xs:restriction>
    		</xs:simpleType>
    	</xs:element>
    	<xs:element name="FirstARTRegimen" type="CodedSimpleType" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">
    				Code and description that represents the first ART regimen the patient was on
    			</xs:documentation>
    		</xs:annotation>
    	</xs:element>
    	<xs:element name="ARTStartDate" type="xs:date" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">Date ART Started</xs:documentation>
    		</xs:annotation>
    	</xs:element>
    	<xs:element name="WHOClinicalStageARTStart" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">
    				Code that represents the WHO Clinical Stage of the patient when ART started
    			</xs:documentation>
    		</xs:annotation>
    		<xs:simpleType>
    			<xs:restriction base="CodeType">
    				<xs:enumeration value="1" />
    				<xs:enumeration value="2" />
    				<xs:enumeration value="3" />
    				<xs:enumeration value="4" />
    				<xs:enumeration value="I" />
    				<xs:enumeration value="II" />
    				<xs:enumeration value="III" />
    				<xs:enumeration value="IV" />

    			</xs:restriction>
    		</xs:simpleType>
    	</xs:element>
    	<xs:element name="WeightAtARTStart" type="xs:int" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">The Patient's Weight in (kg) at ART start</xs:documentation>
    		</xs:annotation>
    	</xs:element>
    	<xs:element name="ChildHeightAtARTStart" type="xs:int" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">The Patient's Height in (cm) if child at ART start</xs:documentation>
    		</xs:annotation>
    	</xs:element>
    	<xs:element name="FunctionalStatusStartART" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">
    				Code that represents the Functional Status of the patient when ART started
    			</xs:documentation>
    		</xs:annotation>
    		<xs:simpleType>
    			<xs:restriction base="CodeType">
    				<xs:enumeration value="W" />
    				<xs:enumeration value="A" />
    				<xs:enumeration value="B" />
    			</xs:restriction>
    		</xs:simpleType>
    	</xs:element>
    	<xs:element name="CD4AtStartOfART" type="StringType" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">CD4 absolute of the patient at the start of ART</xs:documentation>
    		</xs:annotation>
    	</xs:element>
    	<xs:element name="PatientTransferredOut" type="xs:boolean" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">True or false, did the Patient transfer out</xs:documentation>
    		</xs:annotation>
    	</xs:element>
    	<xs:element name="TransferredOutStatus" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">
    				Code that represents ART status when patient transferred out
    			</xs:documentation>
    		</xs:annotation>
    		<xs:simpleType>
    			<xs:restriction base="CodeType">
    				<xs:enumeration value="A" />
    				<xs:enumeration value="P" />
    			</xs:restriction>
    		</xs:simpleType>
    	</xs:element>
    	<xs:element name="TransferredOutDate" type="xs:date" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">Date patient transferred out from this facility</xs:documentation>
    		</xs:annotation>
    	</xs:element>
    	<xs:element name="FacilityReferredTo" type="FacilityType" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">Facility where patient was referred to</xs:documentation>
    		</xs:annotation>
    	</xs:element>
    	<xs:element name="PatientHasDied" type="xs:boolean" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">Did the Patient Die</xs:documentation>
    		</xs:annotation>
    	</xs:element>
    	<xs:element name="StatusAtDeath" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">
    				Code that represents ART status when patient transferred out
    			</xs:documentation>
    		</xs:annotation>
    		<xs:simpleType>
    			<xs:restriction base="CodeType">
    				<xs:enumeration value="A" />
    				<xs:enumeration value="P" />
    			</xs:restriction>
    		</xs:simpleType>
    	</xs:element>
    	<xs:element name="DeathDate" type="xs:date" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">Date patient died</xs:documentation>
    		</xs:annotation>
    	</xs:element>
    	<xs:element name="SourceOfDeathInformation" type="StringType" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">Source of death information</xs:documentation>
    		</xs:annotation>
    	</xs:element>
    	<xs:element name="CauseOfDeathHIVRelated" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">Was cause of death HIV related</xs:documentation>
    		</xs:annotation>
    		<xs:simpleType>
    			<xs:restriction base="CodeType">
    				<xs:enumeration value="Y" />
    				<xs:enumeration value="N" />
    				<xs:enumeration value="U" />
    			</xs:restriction>
    		</xs:simpleType>
    	</xs:element>
    	<xs:element name="CauseOfDeath" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">The cause of death</xs:documentation>
    		</xs:annotation>
    		<xs:simpleType>
    			<xs:restriction base="CodeType">
    				<xs:enumeration value="B24" />
    				<xs:enumeration value="A09" />
    				<xs:enumeration value="B54" />
    				<xs:enumeration value="O95" />
    				<xs:enumeration value="B99" />
    				<xs:enumeration value="J22" />
    				<xs:enumeration value="A16" />
    				<xs:enumeration value="I24" />
    				<xs:enumeration value="C50" />
    				<xs:enumeration value="J44" />
    				<xs:enumeration value="C53" />
    				<xs:enumeration value="K74" />
    				<xs:enumeration value="C18" />
    				<xs:enumeration value="E14" />
    				<xs:enumeration value="C15" />
    				<xs:enumeration value="C96" />
    				<xs:enumeration value="C34" />
    				<xs:enumeration value="I99" />
    				<xs:enumeration value="UU1" />
    				<xs:enumeration value="C61" />
    				<xs:enumeration value="N18" />
    				<xs:enumeration value="C16" />
    				<xs:enumeration value="I64" />
    				<xs:enumeration value="C76" />
    				<xs:enumeration value="X27" />
    				<xs:enumeration value="W74" />
    				<xs:enumeration value="W19" />
    				<xs:enumeration value="X09" />
    				<xs:enumeration value="Y09" />
    				<xs:enumeration value="X58" />
    				<xs:enumeration value="X49" />
    				<xs:enumeration value="V89" />
    				<xs:enumeration value="X84" />
    				<xs:enumeration value="G04" />
    				<xs:enumeration value="A99" />
    				<xs:enumeration value="B05" />
    				<xs:enumeration value="G03" />
    				<xs:enumeration value="A41" />
    				<xs:enumeration value="UU2" />
    				<xs:enumeration value="K92" />
    				<xs:enumeration value="P21" />
    				<xs:enumeration value="Q89" />
    				<xs:enumeration value="P36" />
    				<xs:enumeration value="P23" />
    				<xs:enumeration value="P07" />
    				<xs:enumeration value="P95" />
    				<xs:enumeration value="R99" />
    			</xs:restriction>
    		</xs:simpleType>
    	</xs:element>
    	<xs:element name="DrugAllergies" type="StringType" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">Patient's drug allergies</xs:documentation>
    		</xs:annotation>
    	</xs:element>
    	<xs:element name="EnrolledInHIVCareDate" type="xs:date" minOccurs="1" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">Date initially enrolled in HIV care</xs:documentation>
    		</xs:annotation>
    	</xs:element>
    	<xs:element name="InitialTBStatus" minOccurs="0" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">Code that represents the Initial TB Status of the patient</xs:documentation>
    		</xs:annotation>
    		<xs:simpleType>
    			<xs:restriction base="CodeType">
    				<xs:enumeration value="1" />
    				<xs:enumeration value="2" />
    				<xs:enumeration value="3" />
    				<xs:enumeration value="4" />
    				<xs:enumeration value="5" />
    			</xs:restriction>
    		</xs:simpleType>
    	</xs:element>
    	<xs:element name="StoppedTreatment" type="xs:boolean" minOccurs="0" maxOccurs="1"></xs:element>
    	<xs:element name="DateStoppedTreatment" type="xs:date" minOccurs="0" maxOccurs="1"></xs:element>
    	<xs:element name="ReasonForStoppedTreatment" minOccurs="0" maxOccurs="1">
    		<xs:simpleType>
    			<xs:restriction base="CodeType">
    				<xs:enumeration value="1" />
    				<xs:enumeration value="2" />
    				<xs:enumeration value="3" />
    				<xs:enumeration value="4" />
    			</xs:restriction>
    		</xs:simpleType>
    	</xs:element>
    	<xs:element name="TBTreatmentStartDate" type="xs:date" maxOccurs="1" minOccurs="0"></xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ProgramAreaType">
    <xs:sequence>
      <xs:element name="ProgramAreaCode" type="CodeType"
				minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The Program Area Code
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PatientDemographicsType">
    <xs:sequence>
    	<xs:element name="PatientIdentifier" minOccurs="0"
    		maxOccurs="1">
    		<xs:simpleType>
    			<xs:annotation>
    				<xs:documentation xml:lang="en">
    					The Patient Identifier
    				</xs:documentation>
    			</xs:annotation>
    			<xs:restriction base="xs:string">
    				<xs:minLength value="0" />
    				<xs:maxLength value="2000" />
    			</xs:restriction>
    		</xs:simpleType>
    	</xs:element>
    	<xs:element name="IdentifierChange" type="IdentifierChangeType" minOccurs="0" maxOccurs="1">
    	
    	</xs:element>
    	<xs:element name="TreatmentFacility" type="FacilityType"
    		minOccurs="1" maxOccurs="1">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">
    				The facility where the patient is being treated
    			</xs:documentation>
    		</xs:annotation>
    	</xs:element>
    	<xs:element name="OtherPatientIdentifiers"
    		type="IdentifiersType" minOccurs="0">
    		<xs:annotation>
    			<xs:documentation xml:lang="en">
    				Other Patient Identifiers
    			</xs:documentation>
    		</xs:annotation>
    	</xs:element>
    	<xs:element name="PatientDateOfBirth" type="RestrictedDateType"
    		minOccurs="1" maxOccurs="1">
    		<xs:annotation>
